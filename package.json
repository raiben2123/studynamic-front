    "@capacitor/status-bar": "^7.0.1",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "assert": "^2.0.0",
    "axios": "^1.8.4",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "date-fns": "^3.3.1",
    "dayjs": "^1.11.13",
    "framer-motion": "^11.0.3",
    "https-browserify": "^1.0.0",
    "jetifier": "^2.0.0",
    "lucide-react": "^0.330.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.0",
    "react-scripts": "5.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.3"
  },
  "scripts": {
    "start": "set \"BROWSER=none\" && react-app-rewired start",
    "build": "set \"GENERATE_SOURCEMAP=false\" && react-app-rewired build",
    "build:debug": "set \"GENERATE_SOURCEMAP=true\" && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "cap:init": "npx cap init Studynamic io.studynamic.app",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "build:mobile": "npm run build && npm run cap:sync",
    "setup:capacitor": "node scripts/setup-capacitor.js",
    "clean": "if exist node_modules\\.cache rmdir /s /q node_modules\\.cache && if exist build rmdir /s /q build",
    "build:clean": "npm run clean && npm run build"
  },
  "config-overrides-path": "config-overrides.js",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@capacitor/assets": "^2.0.4",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^10.0.0",
    "core-js": "^3.42.0",
    "postcss": "^8.4.35",
    "react-app-rewired": "^2.2.1",
    "serve": "^14.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-theme-swapper": "^0.12.0",
    "terser-webpack-plugin": "^5.3.14",
    "web-vitals": "^3.5.1"
  }
}
